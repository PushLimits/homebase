{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@icons/*": ["src/components/icons/*"],
      "@images/*": ["src/images/*"],
      "@layouts/*": ["/src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@styles/*": ["/src/styles/*"]
    }
  }
}
